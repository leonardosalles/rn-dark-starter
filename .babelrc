{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "@app/components": "components",
        "@app/static": "static",
        "@app/screens": "screens",
        "@app/routes": "routes",
        "@app/themes": "themes",
        "@app/context": "context"
      }
    }]
  ]

}
